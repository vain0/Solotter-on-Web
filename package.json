{
  "name": "solotter-web",
  "version": "2.0.0",
  "description": "Twitter client webapp for those who wants to stay focused on work",
  "main": "index.js",
  "repository": "https://github.com/vain0x/solotter-web",
  "author": "vain0x <vainzerox@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node ./index.js",
    "test": "run-s prod:server-build prod:test",
    "dev": "run-p dev:*",
    "dev:client-build": "webpack --mode development --watch --progres",
    "dev:server-build": "tsc --watch",
    "dev:server-serve": "nodemon ./index.js --watch ./dist/server",
    "dev:test": "mocha -r ts-node/register ./src/index.spec.ts --watch --watch-extensions ts",
    "prod": "run-s prod:*",
    "prod:client-build": "webpack --mode production",
    "prod:server-build": "tsc",
    "prod:test": "mocha -r ts-node/register ./src/index.spec.ts --exit",
    "prod:server-serve": "node ./index.js"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.11.2",
    "cross-env": "^5.2.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "ts-loader": "^5.3.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1"
  },
  "dependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/csurf": "^1.9.35",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/history": "^4.7.0",
    "@types/oauth": "^0.9.0",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.8",
    "@types/request": "^2.47.1",
    "@types/request-promise-native": "^1.0.15",
    "@types/serve-static": "^1.13.2",
    "@types/twitter-text": "^2.0.0",
    "@types/universal-router": "^6.0.0",
    "@types/uuid": "^3.4.4",
    "body-parser": "^1.18.3",
    "csurf": "^1.9.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "history": "^4.7.2",
    "oauth": "^0.9.15",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "serve-static": "^1.13.2",
    "twitter-text": "^2.0.5",
    "universal-router": "^6.0.0"
  }
}
